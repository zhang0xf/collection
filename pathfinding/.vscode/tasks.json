{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build pathfinding project",
            "type": "shell",
            "command": "make",
            "options": {
                "cwd": "${fileDirname}"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "clean pathfinding project",
            "type": "shell",
            "command": "make clean",
            "options": {
                "cwd": "${fileDirname}"
            },
            "group": {
                "kind": "test",
                "isDefault": false
            }
        }
    ]
}